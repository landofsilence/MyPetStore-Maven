<?xml version="1.0" encoding="UTF-8" ?>
        <!DOCTYPE mapper
                PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
                "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="per.jxnflzc.persistence.CategoryDAO">
<select id="getCategoryList" resultType="per.jxnflzc.domain.Category">
      select * from category
    </select>
<select id="getCategory"  resultType="per.jxnflzc.domain.Category">
        select * from category where catid=#{categoryId}
    </select>

</mapper>